<h1>Listing rooms</h1>

<% @rooms.each do |room| %>
  <div class="field">
    <%= link_to(room.name, room_path(room)) %>
    <br />
    <%= link_to 'Edit', edit_room_path(room) %> |
    <%= link_to 'Destroy', room, :confirm => 'Are you sure?', :method => :delete %>
  </div>
<% end %>

<br />

<%= link_to 'New Room', new_room_path %>
